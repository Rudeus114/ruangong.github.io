<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liang.mapper.DanmuMapper">
    <insert id="addDanmu">
        insert into liang.danmus(text) values (#{text})
    </insert>
    <select id="showDanmu" resultType="com.liang.pojo.Danmu">
        select * from liang.danmus
    </select>
</mapper>